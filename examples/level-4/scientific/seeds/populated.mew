-- Experiments
--# seed_exp_smoking
SPAWN smoking_study: Experiment {
  title = "Longitudinal Smoking Study",
  methodology = "Cohort study",
  sample_size = 10000
}

--# seed_exp_exercise
SPAWN exercise_study: Experiment {
  title = "Exercise and Mortality Meta-Analysis",
  methodology = "Meta-analysis",
  sample_size = 500000
}

