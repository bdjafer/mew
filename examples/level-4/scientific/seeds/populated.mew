-- ===========================================================================
-- SEED: Populated
-- DESCRIPTION: A rich dataset for comprehensive testing
-- ===========================================================================

-- Concepts
--# seed_concept_smoking
SPAWN smoking: Concept {
  name = "Smoking",
  definition = "Regular tobacco consumption",
  domain = "Health Behavior"
}

--# seed_concept_cancer
SPAWN cancer: Concept {
  name = "Lung Cancer",
  definition = "Malignant tumor in lungs",
  domain = "Oncology"
}

--# seed_concept_exercise
SPAWN exercise: Concept {
  name = "Regular Exercise",
  definition = "Physical activity 3+ times per week",
  domain = "Health Behavior"
}

--# seed_concept_longevity
SPAWN longevity: Concept {
  name = "Longevity",
  definition = "Extended lifespan",
  domain = "Health Outcomes"
}

--# seed_concept_stress
SPAWN stress: Concept {
  name = "Chronic Stress",
  domain = "Psychology"
}

-- Hypotheses
--# seed_hyp_smoking
SPAWN smoking_hyp: Hypothesis {
  statement = "Smoking causes lung cancer",
  status = "established"
}

--# seed_hyp_exercise
SPAWN exercise_hyp: Hypothesis {
  statement = "Regular exercise increases longevity",
  status = "supported"
}

--# seed_hyp_stress
SPAWN stress_hyp: Hypothesis {
  statement = "Chronic stress reduces longevity",
  status = "proposed"
}

-- Researchers
